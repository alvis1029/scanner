<launch>

    <!-- 開啟yolov4 -->
    <include file="$(find yolov4)/launch/yolov4_person.launch">
    </include>

    <!-- 開啟點雲濾波器 -->
    <node name="scan_person_filter_node" pkg="scanner" type="scan_person_filter_node" required="true" output="screen" />

    <!-- 開啟點雲分群器已取得深度資訊 -->
    <node name="scan_person_clustering_node" pkg="scanner" type="scan_person_clustering_node" required="true" output="screen"/>

    <!-- 開啟2D_guassian -->
    <node pkg="scanner" name="Setting_Max_Speed" type="2D_gaussian.py" output="screen" required="true"/>

</launch>